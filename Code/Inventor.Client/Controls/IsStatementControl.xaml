<UserControl x:Class="Inventor.Client.Controls.IsStatementControl"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<StackPanel Orientation="Vertical" Name="_contextControl">
		<GroupBox Name="_groupParent">
			<ComboBox Name="_comboBoxParent" SelectedItem="{Binding Path=Parent, Mode=TwoWay}" />
		</GroupBox>
		<GroupBox Name="_groupChild">
			<ComboBox Name="_comboBoxChild" SelectedItem="{Binding Path=Child, Mode=TwoWay}" />
		</GroupBox>
	</StackPanel>
</UserControl>
