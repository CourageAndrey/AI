<UserControl x:Class="Inventor.Client.Controls.HasSignStatementControl"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<StackPanel Orientation="Vertical" Name="_contextControl">
		<GroupBox Header="Concept">
			<ComboBox Name="_comboBoxConcept" SelectedItem="{Binding Path=Concept, Mode=TwoWay}" />
		</GroupBox>
		<GroupBox Header="Sign">
			<ComboBox Name="_comboBoxSign" SelectedItem="{Binding Path=Sign, Mode=TwoWay}" />
		</GroupBox>
	</StackPanel>
</UserControl>
