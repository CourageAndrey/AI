<Window x:Class="AabSemantics.Extensions.WPF.Dialogs.SelectModulesDialog"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		SizeToContent="WidthAndHeight" MinHeight="200" MinWidth="100" WindowStartupLocation="CenterOwner">
	<DockPanel LastChildFill="True">
		<StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" Margin="5">
			<Button Margin="5" Padding="5,2,5,2" Name="_buttonOk" Click="okClick">OK</Button>
			<Button Margin="5" Padding="5,2,5,2" Name="_buttonCancel" Click="cancelClick">Cancel</Button>
		</StackPanel>
		<StackPanel Orientation="Vertical" Name="_groupBox" Margin="5" />
	</DockPanel>
</Window>
