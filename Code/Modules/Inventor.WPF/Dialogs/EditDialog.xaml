<Window x:Class="Inventor.WPF.Dialogs.EditDialog"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		SizeToContent="WidthAndHeight" WindowStartupLocation="CenterOwner">
	<DockPanel LastChildFill="True" Name="_dockPanel">
		<StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" Margin="5">
			<Button Margin="5" Padding="5,2,5,2" Name="_buttonOk" Click="okClick">OK</Button>
			<Button Margin="5" Padding="5,2,5,2" Name="_buttonCancel" Click="cancelClick">Cancel</Button>
		</StackPanel>
	</DockPanel>
</Window>
